package com.jasson.assignment;

public class CustomerId {
	
	private String id;
	private String name;
	public CustomerId() {
		super();
	}
	public CustomerId(String name, String id) {
		super();
		this.name = name;
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	@Override
	public String toString() {
		return "CustomerId [name=" + name + ", id=" + id + "]";
	}  
	
	
	
	
}

